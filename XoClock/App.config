<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
	<appSettings>
		<!--FontColor sets color of text Red-Electro:FFDF0C28 Blue:2879FF-->
		<add key="FontColor" value="#2879FF"/>
		<!--TextDropShadowColor color used to highlight time and date -->
		<add key="TextDropShadowColor" value="#99c0ff"/>
		<!--Uncomment BgColor and Comment BgImage to set specific color for background-->
		<!--<add key="BgColor" value="#FF0000"/>-->
		<!--BgImage sets background image for the clock, if it is set, BgColor is ignored-->
		<add key="BgImage" value="BgImage\black-gradient.jpg"/>
		<!-- Corner for bottom (not settable for top)-->
		<add key="CornerRadius" value="20"/>
		<!--BackgroundOpacity: 
			0   = totally transparent (you see only numbers)
			1   = totally visible 
			0.5 = half visible -->
		<add key="BackgroundOpacity" value="1"/>
	</appSettings>
</configuration>